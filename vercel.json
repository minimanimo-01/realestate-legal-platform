{
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  },
  "installCommand": "npm install --force", // 강제로 의존성 설치
  "buildCommand": "npm run build",        // 빌드 실행
  "rewrites": [
    {
      "source": "/assets/(.*)",
      "destination": "/assets/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
